<Aircraft>
  <Reference>
    <RefArea>1.0</RefArea>
    <RefLength>1.0</RefLength>
    <MomentCenX>0.0</MomentCenX>
    <MomentCenY>0.0</MomentCenY>
    <MomentCenZ>0.0</MomentCenZ>
  </Reference>
  <Wing name="Main">
    <Paneling>
      <NChord>61</NChord>
      <NSpan>31</NSpan>
      <LESpaceRat>0.2</LESpaceRat>
      <TESpaceRat>0.4</TESpaceRat>
      <RootSpaceRat>0.75</RootSpaceRat>
      <TipSpaceRat>0.4</TipSpaceRat>
    </Paneling>
    <Sections>
      <!-- Note that sections do not need to be given in order -->
      <Section name="Root">
        <!-- Y is not needed for root section -->
        <XLE>0.</XLE>
        <ZLE>0.</ZLE>
        <Chord>9</Chord>
        <Twist>0.</Twist>
      </Section>
      <Section>
        <XLE>3.0</XLE>
        <Y>16.0</Y>
        <ZLE>1.5</ZLE>
        <Chord>6.0</Chord>
        <Twist>-2.581</Twist>
      </Section>
      <Section>
        <XLE>2.0</XLE>
        <Y>8.0</Y>
        <ZLE>0.0</ZLE>
        <Chord>7.0</Chord>
        <Twist>0.</Twist>
      </Section>
      <Section>
        <XLE>4.2</XLE>
        <Y>23.1</Y>
        <ZLE>2.8313</ZLE>
        <Chord>4.8</Chord>
        <Twist>-4.871</Twist>
      </Section>
      <Section>
        <XLE>4.0</XLE>
        <Y>22.7</Y>
        <ZLE>2.756</ZLE>
        <Chord>5.0</Chord>
        <Twist>-4.742</Twist>
      </Section>
      <Section>
        <!-- The tip section is the one with largest Y -->
        <XLE>4.8</XLE>
        <Y>23.5</Y>
        <ZLE>2.90625</ZLE>
        <Chord>4.2</Chord>
        <Twist>-5.</Twist>
      </Section>
    </Sections>
    <Airfoils>
      <Airfoil>
        <Source>5 digit</Source>
        <Designation>21008</Designation>
        <Y>0.</Y>
      </Airfoil>
      <Airfoil name="Tip">
        <!-- Note: since we named this airfoil "Tip", we do not need to provide
             a Y value. The same applies with "Root". -->
        <Source>4 digit</Source>
        <Designation>0006</Designation>
      </Airfoil>
      <Airfoil>
        <!-- Y is needed for non-root / non-tip airfoils -->
        <Source>file</Source>
        <Path>clarky.dat</Path>
        <Y>10</Y>
      </Airfoil>
    </Airfoils>
  </Wing>
  <Wing name="Tail">
    <Paneling>
      <NChord>31</NChord>
      <NSpan>21</NSpan>
      <LESpaceRat>0.2</LESpaceRat>
      <TESpaceRat>0.4</TESpaceRat>
      <RootSpaceRat>0.75</RootSpaceRat>
      <TipSpaceRat>0.4</TipSpaceRat>
    </Paneling>
    <Sections>
      <Section name="Root">
        <!-- Y is not needed for root section -->
        <XLE>22.</XLE>
        <ZLE>-3.</ZLE>
        <Chord>4.</Chord>
        <Twist>0.</Twist>
      </Section>
      <Section>
        <XLE>24.</XLE>
        <Y>6.</Y>
        <ZLE>-3.</ZLE>
        <Chord>3.</Chord>
        <Twist>0.</Twist>
      </Section>
    </Sections>
    <Airfoils>
      <!-- If airfoil is constant, only need to supply root or tip -->
      <Airfoil name="Root">
        <Source>4 digit</Source>
        <Designation>0005</Designation>
      </Airfoil>
    </Airfoils>
  </Wing>
</Aircraft>
